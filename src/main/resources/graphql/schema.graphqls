type Query {
    userById(id: ID): User
    getAllUsers: [User!]!
}

type Mutation {
    createUser(user: UserInput): User
}

type User {
    id: ID!
    username: String!
    email: String
    birthDate: String!
    images: [Image!]!
}

input UserInput {
    username: String!
    email: String
    birthDate: String!
}

type Image {
    id: ID!
    originalName: String!
    size: String!
    link: String!
}